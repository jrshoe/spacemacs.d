;;; -*- lexical-binding: t; -*-
(provide 'prettify-redex-alist)
(require 'prettify-utils)
(defvar prettify-redex-alist
  (prettify-utils-generate
	 ("|>"		"▷")
	 ("<|"		"◁")
	 ("->>"		"↠")
	 ("->"		"→")
	 ("<-"		"←")
	 ("=>"		"⇒")
	 ("<="		"⇐")
   ("|->"   "↦")
   ("tau"   "τ")
   ("dot" "·")
   ("comma" ",")
   ("Gamma" "Γ")
   ("gamma" "γ")
   ("psi" "ψ")
   ("Psi" "Ψ")
   ("lambda" "λ")
   ("Lambda" "Λ")
   ("doublecomma" "„")
   ("|-"  "⊢")
   ("-->" "⟶")
   ))
